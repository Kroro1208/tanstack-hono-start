{
  "name": "@{{projectName}}/web",
  "version": "0.1.0",
  "description": "Frontend app built with React and TanStack Router",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3000",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"{{#if features.vitest}},
    "test": "vitest",
    "test:ui": "vitest --ui"{{/if}}{{#if features.linting}},
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"{{/if}}
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@tanstack/react-router": "^1.15.0",
    "@tanstack/router-devtools": "^1.15.0",
    "zod": "^3.22.0",
    "@tanstack/react-query": "^5.17.0",
    "clsx": "^2.1.0"{{#if features.auth}},
    "@auth/core": "^0.18.0"{{/if}}
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@tanstack/router-vite-plugin": "^1.15.0",
    "vite": "^5.0.0",
    "typescript": "^5.3.0"{{#if features.tailwind}},
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"{{/if}}{{#if features.vitest}},
    "vitest": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "jsdom": "^23.0.0"{{/if}}{{#if features.linting}},
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5"{{/if}}
  }
}